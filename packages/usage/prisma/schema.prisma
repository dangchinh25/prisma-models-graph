generator custom_generator {
  provider = "npx prisma-models-graph"
  output   = "../types"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?

  @@map("users")
}

model Post {
  id      Int    @id @default(autoincrement())
  subject String
  body    String
  userId  Int    @map("user_id") /// [[User.id]]

  @@map("posts")
}

enum NotificationType {
  newPosts
  newComments
  newFollowers
  reply
  heartOnPost
  heartOnComment
  heartOnReply
}

enum Language {
  Typescript
  Javascript
  Rust
  Go
  Python
  Cpp
}
